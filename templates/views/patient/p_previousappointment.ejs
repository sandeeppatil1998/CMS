<body class="g-sidenav-show  bg-gray-200">
    <!-- SideNav -->
    <%- include('../../partials/patient/p_appointment_sidenav'); -%>

        <main class="main-content position-relative max-height-vh-100 h-100 border-radius-lg ">

            <!-- Navbar -->

            <%- include('../../partials/navbar'); -%>

                <div class="container-fluid py-4">
                    <div class="row">
                        <div class="col-12">
                            <div class="card my-4">
                                <div class="card-header p-0 position-relative mt-n4 mx-3 z-index-2">
                                    <div class="bg-gradient-primary shadow-primary border-radius-lg pt-4 pb-3">
                                        <h6 class="text-white text-capitalize ps-3">Previous Appointment List</h6>
                                        <!-- <span class="badge badge-sm bg-gradient-success" style="position: absolute; top: 34%; right: 7%;">Add Patient</span> -->
                                    </div>
                                </div>
                                <div class="card-body px-0 pb-2">
                                    <div class="table-responsive p-0">
                                        <div class="card-header pb-0 px-3"
                                            style="display: flex; justify-content: space-between;">
                                            <div>
                                                <h6 class="mb-0">Appointment Information</h6>
                                            </div>
                                            <div style="margin-right: 3%;">
                                                <button class="btn btn-primary"
                                                    style="font-size: large;" onclick="history.back()">Back</button>
                                            </div>
                                        </div>
                                        <% if(allprescription.length==0) {%>
                                            <h1 style="text-align: center; color: #c9c2c2;"> No Previous Records</h1>
                                            <% }else{ %>
                                                <% for(i=0;i<allprescription.length;i++){ %>
                                                    <div class="card-body pt-4 p-3">
                                                        <ul class="list-group">
                                                            <li class="list-group-item border-0 p-4 mb-2 bg-gray-100 border-radius-lg">
                                                                <div class="d-flex flex-column flex-sm-row justify-content-between" >
                                                                    <div class="mb-2 mb-sm-0">
                                                                        <h5 class="mb-0">Patient ID:</h5>
                                                                        <input type="text" class="form-control" style="background: none;" value="<%= allprescription[i].patient_id %>" readonly>
                                                                    </div>
                                                                    <div class="mb-2 mb-sm-0">
                                                                        <h5 class="mb-0">Patient Name:</h5>
                                                                        <input type="text" class="form-control" style="background: none;" value="<%= allprescription[i].patient_name %>" readonly>
                                                                    </div>
                                                                    <div class="mb-2 mb-sm-0">
                                                                        <h5 class="mb-0">Date:</h5>
                                                                        <input type="text" class="form-control" style="background: none;" value="<%= (allprescription[i].prescription_date).toString().slice(0, 15) %>" readonly>
                                                                    </div>
                                                                </div>
                                                                <br>
                                                                <div class="d-flex flex-column flex-sm-row justify-content-between" >
                                                                    <div class="mb-2 mb-sm-0">
                                                                        <h5 class="mb-0">Appointment ID:</h5>
                                                                        <input type="text" class="form-control" style="background: none;" value="<%= allprescription[i].id %>" readonly>
                                                                    </div>
                                                                    <div class="mb-2 mb-sm-0">
                                                                        <h5 class="mb-0">Doctor Name:</h5>
                                                                        <input type="text" class="form-control" style="background: none;" value="<%= allprescription[i].doctor_name %>" readonly>
                                                                    </div>
                                                                    <div class="mb-2 mb-sm-0">
                                                                        <h5 class="mb-0">Compliant:</h5>
                                                                        <input type="text" class="form-control" style="background: none;" value="<%= allprescription[i].complain %>" readonly>
                                                                    </div>
                                                                </div>
                                                            </li>
                                                        </ul>
                                                    </div>
                                                <% } %>
                                            <% } %>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
        </main>


