			const departmentLinks = $('.department-wrap a');

			// Attach a click event to each department link
			departmentLinks.click(function (e) {
				e.preventDefault();

				// Get the target department ID from the link's href
				const targetDepartmentID = $(this).attr('href');

				// Update the carousel to the target department
				$('.carousel').carousel('pause'); // Pause the carousel
				$('.carousel .carousel-item').removeClass('active'); // Remove active class from all items
				$(targetDepartmentID).addClass('active'); // Add active class to the target department

				// Scroll to the top of the target section
				$('html, body').animate({
					scrollTop: $(targetDepartmentID).offset().top
				}, 300); // You can adjust the animation duration (1000ms) as needed

			});
		});
	</script>

